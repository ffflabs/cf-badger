{
  "name": "@treispa/edge-resizer",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "main": "dist/worker.js",
  "types": "types/main.d.ts",
  "scripts": {
    "deploy": "wrangler publish",
    "dev": "wrangler dev --env=dev",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'",
    "eslint": "eslint --ext js --ext ts --ignore-path .eslintignore {src,test}/*.{js,ts}",
    "test:clean": "rimraf ./transpiled/src ./transpiled/test",
    "build": "node esbuild.mjs",
    "toml2yaml": "echo 'cat wrangler.toml | yj -ty > wrangler.yaml'",
    "yaml2toml": "cat wrangler.yaml | yj -yt -i > wrangler.toml",
    "miniflare": "NODE_ENV=development miniflare dist/worker.js -c ./wrangler.toml -w  -d --wrangler-env=dev ",
    "test": "npm run test:clean && npm run transpile && mocha --require source-map-support/register --recursive transpiled/test",
    "transpile": "tsc --project ./test"
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "@babel/core": "7",
    "@babel/eslint-parser": "^7.12.1",
    "@cloudflare/workers-types": "^2.0.0",
    "@sentry/node": "^6.2.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "esbuild": "^0.12.22",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "miniflare": "^1.3.3",
    "mocha": "^8.0.1",
    "node-fetch": "^2.6.1",
    "node-fetch-response-matchers": "^1.1.47",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "service-worker-mock": "^2.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@cfworker/sentry": "^1.8.0",
    "@cfworker/web": "^1.8.0",
    "itty-router": "^2.4.2",
    "toucan-js": "^2.5.0"
  }
}
