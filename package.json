{
  "name": "@treispa/edge-resizer",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "main": "dist/worker.js",
  "types": "types/main.d.ts",
  "scripts": {
    "deploy": "yarn yt &&  yarn write_commit && yarn eslint && yarn transpile && wrangler publish",
    "tail":"wrangler tail --format=pretty",
    "dev": "wrangler dev --env=dev",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'",
    "eslint": "eslint --ext js --ext ts --ignore-path .eslintignore src/*.ts src/modules/*.ts",
    "allexport": " set -o allexport; source .env.local; set +o allexport",
    "localtest":"yarn allexport;yarn test",
    "test": "cp wrangler.example.toml wrangler.toml; wrangler build ; node  --experimental-vm-modules `yarn bin`/ava test/handler.test.mjs ",
    "build": "node esbuild.mjs",
    "ty": "echo 'cat wrangler.toml | yj -ty > wrangler.yaml'",
    "write_commit": "COMMIT=`git rev-parse --short HEAD` sed -i \"s/_RELEASE_/$COMMIT/g\" wrangler.toml",
    "yt": "cat wrangler.yaml | yj -yt -i > wrangler.toml",
    "miniflare": "yarn yt && yarn write_commit && NODE_ENV=development miniflare dist/worker.js -c ./wrangler.toml -w  -d --wrangler-env=dev ",
    "transpile": "tsc --project ./tsconfig.json",
    "act":"yarn allexport; act push -s PERSONAL_GITHUB_TOKEN=$GITHUB_TOKEN"
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "@babel/core": "7",
    "@babel/eslint-parser": "^7.12.1",
    "@cloudflare/workers-types": "^2.0.0",
    "@sentry/node": "^6.2.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "ava": "^3.15.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "esbuild": "^0.12.22",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "miniflare": "~1.4.1",
    "mocha": "^8.0.1",
    "node-fetch": "^2.6.1",
    "node-fetch-response-matchers": "^1.1.47",
    "prettier": "^2.2.1",
    "service-worker-mock": "^2.0.5",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.3",
    "itty-router": "^2.4.2",
    "itty-router-extras": "^0.4.2",
    "toucan-js": "^2.5.0"
  }
}
